{
    "env": {
        "browser": true,
        "node": false
    },
    "extends": [
        /** @see https://eslint.vuejs.org/rules/ */
        "plugin:vue/vue3-recommended"
    ],
    "parserOptions": {
        "parser": "@typescript-eslint/parser",
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "vue/html-indent": "off",
        "vue/max-attributes-per-line": "off",
        "vue/singleline-html-element-content-newline": "off"
    }
}
